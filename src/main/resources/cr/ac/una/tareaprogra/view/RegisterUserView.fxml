<AnchorPane id="AnchorPane" prefHeight="450.0" prefWidth="675.0" styleClass="anchorPane-Center" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cr.ac.una.tareaprogra.controller.RegisterUserViewController">
   <children>
